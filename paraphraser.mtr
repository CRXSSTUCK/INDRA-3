;;; to massage MRSes before generation

anjing_kucing := monotonic_omtr &
[ INPUT.RELS <! [ PRED "_anjing_n_rel" ] !>,
  OUTPUT.RELS <! +copy+ & [ PRED "_kucing_n_rel" ] !> ].

